@using SisRent.Vista.Areas.Mantencion.Models
@{
    //Validar Inicio
    if (Session["HeaderViewModel"] == null)
    {
        Response.Redirect(Url.Action("Index", "Inicio", new { area = "Mantencion" }));
    }

    // Preconfigs
    const string colorSkin = "green";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>SisRent - Mantención - @ViewBag.Title</title>
    @Styles.Render("~/Styles/AdminLTE")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("Styles", required: false)
</head>
<body class="hold-transition skin-@colorSkin sidebar-mini">
    <div class="wrapper">
        @Html.Partial("Partials/Header", (HeaderViewModel)Session["HeaderViewModel"])
        @Html.Partial("Partials/Sidebar", (SidebarViewModel)Session["SidebarViewModel"])
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
            </div>
            <strong>Copyright &copy; 2014-@DateTime.Now.Year <a href="https://adminlte.io">Almsaeed Studio</a>.</strong> All rights
            reserved.
        </footer>
    </div>
    @Scripts.Render("~/Scripts/AdminLTE")
    @RenderSection("Scripts", required: false)
</body>
</html>